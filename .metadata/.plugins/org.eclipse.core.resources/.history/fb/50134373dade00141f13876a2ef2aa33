			BufferedWriter in1 = new BufferedWriter(new FileWriter("home\\enigmatus\\BTP\\BTP_data_main\\acmmapmiscclearnewfornocomma.txt"));			
			String line="";
			String temp1,temp2;
			StringTokenizer st1=null;
